                                                a(href='#', data-filter=category) #{category}
                        .row
                            if certificates.length === 0
                                p.mb-200 Comming Soon
                            - var i = 0
                            while i < certificates.length
                                - var j = i;
                                - i+=2;
                                .col-md-12
                                    ul.work.mb-20
                                        while  j < Math.min(i, certificates.length)
                                            li.col-sm-6
                                                div.item(class=certificates[j].category)
                                                    a
                                                        .desc
                                                            h5.proj-desc
                                                                | #{certificates[j].title}
                                                                span #{certificates[j].date}
                                                        ul#block-slider-certification
                                                            each image in certificates[j++].images
                                                                li
                                                                    a.popup(href=image.secure_url)
                                                                        img(src=image.secure_url, alt='')
